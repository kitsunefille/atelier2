void animateScreen() {
  // the pet's status is checked every time animateScreen() is called
  if (petStatus == 0) { //IF PET IS HAPPY, DO THIS CODE:
    u8g2.clearBuffer();
    happyFrame1();
    u8g2.sendBuffer();
    delay(1000/framesPerSecond);

    u8g2.clearBuffer();
    happyFrame2();
    u8g2.sendBuffer();
    delay(1000/framesPerSecond);

    u8g2.clearBuffer();
    happyFrame3();
    u8g2.sendBuffer();
    delay(1000/framesPerSecond);

    u8g2.clearBuffer();
    happyFrame2();
    u8g2.sendBuffer();
    delay(1000/framesPerSecond);
